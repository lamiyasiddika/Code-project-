#include <stdio.h>
int det2(int m[2][2]) { return m[0][0]*m[1][1]-m[0][1]*m[1][0]; }
int det3(int m[3][3]) {
    int d = m[0][0]*(m[1][1]*m[2][2]-m[1][2]*m[2][1])
          - m[0][1]*(m[1][0]*m[2][2]-m[1][2]*m[2][0])
          + m[0][2]*(m[1][0]*m[2][1]-m[1][1]*m[2][0]);
    return d;
}
int main(){
    int n; scanf("%d",&n);
    if(n==2){ int m[2][2]; for(int i=0;i<2;i++) for(int j=0;j<2;j++) scanf("%d",&m[i][j]);
        printf("%d\n", det2(m)); }
    else if(n==3){ int m[3][3]; for(int i=0;i<3;i++) for(int j=0;j<3;j++) scanf("%d",&m[i][j]);
        printf("%d\n", det3(m)); }
    else printf("Only 2x2 or 3x3 supported\n");
    return 0;
}
