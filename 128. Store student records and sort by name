#include <stdio.h>
#include <string.h>
struct Student{ char name[50]; int id; };
int main(){
    struct Student s[3]={{"Zara",2},{"Ali",3},{"Babu",1}};
    int n=3;
    for(int i=0;i<n-1;i++) for(int j=i+1;j<n;j++)
        if(strcmp(s[i].name,s[j].name)>0){ struct Student t=s[i]; s[i]=s[j]; s[j]=t; }
    for(int i=0;i<n;i++) printf("%s %d\n", s[i].name, s[i].id); return 0;
}
