#include <stdio.h>
#include <string.h>
void rev(char *s){ int i=0,j=strlen(s)-1; while(i<j){char t=s[i]; s[i]=s[j]; s[j]=t; i++; j--; } }
int main(){
    char a[256], b[256], res[257];
    fgets(a,256,stdin); fgets(b,256,stdin);
    a[strcspn(a,"\n")]=0; b[strcspn(b,"\n")]=0;
    rev(a); rev(b);
    int n = strlen(a), m = strlen(b), carry=0, k=0;
    for(int i=0;i< (n>m?n:m) || carry;i++){
        int da = (i<n)? a[i]-'0':0;
        int db = (i<m)? b[i]-'0':0;
        int sum = da + db + carry;
        res[k++] = (sum%2)+'0';
        carry = sum/2;
    }
    res[k]=0; rev(res);
    printf("%s\n", res); return 0;
}
