#include <stdio.h>
#include <stdlib.h>
int main(){
    int n,m; scanf("%d %d",&n,&m);
    int a[n], b[m];
    for(int i=0;i<n;i++) scanf("%d",&a[i]);
    for(int j=0;j<m;j++) scanf("%d",&b[j]);
    for(int i=0;i<n;i++)
        for(int j=0;j<m;j++)
            if(a[i]==b[j]) { printf("%d ", a[i]); break; }
    printf("\n"); return 0;
}
