#include <stdio.h>

void reverse(int arr[], int left, int right){
    if(left >= right) return;
    int temp = arr[left];
    arr[left] = arr[right];
    arr[right] = temp;
    reverse(arr, left+1, right-1);
}

int main() {
    int n;
    scanf("%d",&n);

    int arr[n];
    for(int i=0;i<n;i++) scanf("%d",&arr[i]);

    reverse(arr, 0, n-1);

    for(int i=0;i<n;i++) printf("%d ", arr[i]);
}
