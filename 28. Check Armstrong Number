#include <stdio.h>
#include <math.h>

int main() {
    int n, temp, d=0, sum=0;
    scanf("%d",&n);

    temp = n;
    while (temp) { d++; temp /= 10; }

    temp = n;
    while (temp) {
        int r = temp % 10;
        sum += pow(r, d);
        temp /= 10;
    }

    (sum == n) ? printf("Armstrong") : printf("Not Armstrong");
    return 0;
}
